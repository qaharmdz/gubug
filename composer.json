{
    "name": "qaharmdz/gubug",
    "description": "Pragmatic PHP micro-framework",
    "keywords": ["micro-framework"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Mudzakkir",
            "email": "qaharmdz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "pimple/pimple": "^3.0",
        "symfony/http-foundation": "^4.0",
        "symfony/routing": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "require-dev": {
        "kint-php/kint": "^2.0",
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^3.2",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": { "Gubug\\": "src/" }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable" : true
}
